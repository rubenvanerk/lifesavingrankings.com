{
    "name": "rubenvanerk/lifesavingrankings.com",
    "type": "project",
    "description": "LifesavingRankings.com",
    "keywords": ["laravel", "lifesaving", "results"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ruben van Erk",
            "email": "ruben@lifesavingrankings.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "bensampo/laravel-enum": "^4.0",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "blade-ui-kit/blade-ui-kit": "^0.3.1",
        "diglactic/laravel-breadcrumbs": "^7.0",
        "filament/filament": "^2.0",
        "filament/spatie-laravel-media-library-plugin": "^2.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.75",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.5",
        "rinvex/countries": "^7.3",
        "smalot/pdfparser": "dev-master",
        "spatie/laravel-medialibrary": "^9.11",
        "spatie/laravel-permission": "^5.4",
        "spatie/laravel-sluggable": "^3.1",
        "spatie/regex": "^3.1",
        "staudenmeir/eloquent-eager-limit": "^1.0",
        "stijnvanouplines/blade-country-flags": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "barryvdh/laravel-debugbar": "dev-master",
        "barryvdh/laravel-ide-helper": "^2.10",
        "nunomaduro/larastan": "^1.0",
        "laravel/sail": "^1.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan filament:upgrade"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:rubenvanerk/pdfparser.git"
        }
    ]
}
